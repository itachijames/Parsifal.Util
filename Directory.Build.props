<Project>

	<PropertyGroup>
		<Product>Parsifal.Util</Product>
		<Authors>itachijames</Authors>
		<Company>itachijames</Company>
		<Copyright>Â© ITACHIJAMES. All Rights Reserved.</Copyright>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/itachijames/Parsifal.Util</PackageProjectUrl>
		<RepositoryUrl>https://github.com/itachijames/Parsifal.Util.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
